{
  "name": "blsv-space/inquisition-php",
  "version": "0.3.2",
  "description": "description",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "silex-rr"
    }
  ],
  "require": {
    "php": ">=8.5",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "ext-ctype": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2.0",
    "vimeo/psalm": "^6.15",
    "friendsofphp/php-cs-fixer": "^3.93",
    "psalm/plugin-phpunit": "^0.19.5"
  },
  "autoload": {
    "psr-4": {
      "Inquisition\\": "src/"
    },
    "files": [
      "src/Foundation/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Inquisition\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "psalm": "psalm",
    "cs:check": "php-cs-fixer fix --dry-run --diff",
    "cs:fix": "php-cs-fixer fix"
  }
}
